{
  "expo": {
    "name": "Leancure-view ",
    "slug": "leancure-view",
    "version": "2.0.0",
    "orientation": "default",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "L'application souhaite accéder à vos photos."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/Loop.png",
          "color": "#ffffff",
          "sounds": [],
          "mode": "default",
          "androidMode": "default",
          "androidCollapsedTitle": "Nouvelle notification",
          "firebaseCloudMessaging": {
            "enabled": true
          }
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-file-system",
      "expo-media-library",
      "expo-localization",
      [
        "./plugins/withAndroidSecurityConfig"
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.leancure.app.v2",
      "adaptiveIcon": {
        "backgroundColor": "#e5e5e5"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "googleServicesFile": ".secrets/google-services.json"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
      },
      "apiUrl": "http://VOTRE_IP/ic.php"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "developmentClient":  {}
  }
}
